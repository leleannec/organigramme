                <artifactId>maven-pmd-plugin</artifactId>
                <version>3.4</version>
            </plugin>
        </plugins>
                
    </reporting>

    
    <!-- audit de code : profile spécifique -->
    <profiles>
        <profile>
            <!-- activation directe : "mvn -P audit" -->
            <id>audit</id>
            <build>
                <plugins>
                    <!-- stress test (Lancement via "mvn -P audit verify") -->
                    <plugin>
                        <groupId>com.lazerycode.jmeter</groupId>
                        <artifactId>jmeter-maven-plugin</artifactId>
                        <version>1.10.1</version>
                        <executions>
                            <execution>
                                <id>audit-verify-jmeter</id>
                                <phase>verify</phase>
                                <goals>
                                    <goal>jmeter</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
            <reporting>
                <plugins>
                    <!-- analyse de code (Lancement via "mvn -P audit site") -->
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>codenarc-maven-plugin</artifactId>
                        <version>0.18.1-1-SNAPSHOT</version>
                    </plugin>
                </plugins>
            </reporting>
        </profile>
    </profiles>

</project>
